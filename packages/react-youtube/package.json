{
  "name": "@youtube",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pnpm build:postcss && pnpm build:swc && pnpm types",
    "build:swc": "swc src -d dist --copy-files",
    "build:postcss": "postcss src/index.css -o src/styles.css --minify",
    "dev": "pnpm build:swc -w && pnpm dev:postcss",
    "dev:postcss": "postcss src/index.css -o src/styles.css --watch",
    "types": "tsc --emitDeclarationOnly",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf dist && rm -rf .turbo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "lucide-react": "^0.115.0",
    "tailwind-merge": "^1.9.1",
    "tailwindcss": "^3.2.4",
    "tailwindcss-animate": "^1.0.5"
  },
  "devDependencies": {
    "tsup": "^7.1.0",
    "tsconfig": "workspace:*",
    "tailwind-config": "workspace:*",
    "react": "^18.2.0",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.66"
  },
  "typesVersions": {
    "*": {
      "components/ui/Youtube": [
        "dist/components/ui/Youtube.d.mts"
      ],
      "core/functions": [
        "dist/core/functions.d.mts"
      ],
      "Youtube": [
        "dist/Youtube.d.mts"
      ],
      "functions": [
        "dist/functions.d.mts"
      ],
      "components/ui/YTvideoPlayer": [
        "dist/components/ui/YTvideoPlayer.d.mts"
      ],
      "FetchData": [
        "dist/FetchData.d.mts"
      ],
      "FetchPlaylist": [
        "dist/FetchPlaylist.d.mts"
      ],
      "ui/Youtube": [
        "dist/ui/Youtube.d.mts"
      ],
      "core/FetchPlaylist": [
        "dist/core/FetchPlaylist.d.mts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    "./styles.css": "./dist/index.css",
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./ui/Youtube": {
      "import": "./dist/ui/Youtube.mjs",
      "types": "./dist/ui/Youtube.d.mts"
    },
    "./core/FetchPlaylist": {
      "import": "./dist/core/FetchPlaylist.mjs",
      "types": "./dist/core/FetchPlaylist.d.mts"
    }
  }
}